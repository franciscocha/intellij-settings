<keymap version="1" name="Default copy" parent="$default">
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="shift ctrl d" />
  </action>
  <action id="Images.SetBackgroundImage">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="SelectNextOccurrence">
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
</keymap>